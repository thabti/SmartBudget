{
  "name": "SmartBudget",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test mocha src/ --recursive --glob *-spec.js --require ./web/tools/test/helpers/browser.js --require ./web/tools/native/index.js --require ignore-styles --colors",
    "test:watch": "npm run test -- --watch",
    "start": "react-native start",
    "ios:bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios:bundle:dev": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android:bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:bundle:dev": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "web:bundle": "webpack --config web/tools/webpack/webpack.config.prod.js --progress --colors",
    "web:dev": "webpack-dev-server --content-base public/ --config web/tools/webpack/webpack.config.dev.js --port 4000 --inline --hot --colors",
    "clear-cache": "gulp clear-cache"
  },
  "dependencies": {
    "axios": "0.15.3",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-native": "0.41.2",
    "react-native-web": "0.0.70",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-saga": "0.14.3"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-umd": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.18.0",
