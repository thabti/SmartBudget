{
  "name": "SmartBudget",
  "version": "0.0.1",
  "private": true,
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "**/Application/**/*.js",
      "**/Application/*.js"
    ],
    "exclude": [
      "**/__tests__/**",
      "**/.nyc_output/**",
      "**/_example/**",
      "**/coverage/**",
      "index.android.js",
      "index.ios.js",
      "index.web.js",
      "**/*.scss",
      "**/*.png",
      "**/*.ico",
      "**/*.spec.js",
      "**/web/**/*",
      "**/ios/**/*",
      "**/android/**/*",
      "**/node_modules/**"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "extension": [
      ".js"
    ],
    "cache": false,
    "sourceMap": false,
    "instrument": false,
    "report-dir": "./coverage"
  },
  "scripts": {
    "coverage:check": "cross-env NODE_ENV=test nyc check-coverage",
    "coverage": "cross-env NODE_ENV=test nyc npm test",
    "test": "cross-env NODE_ENV=test mocha Application/ --recursive --glob *.spec.js --require ./web/tools/test/helpers/browser.js --require ignore-styles --colors",
    "test:watch": "npm run test -- --watch",
    "start": "react-native start",
    "ios:bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios:bundle:dev": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios:run:dev": "npm run ios:bundle:dev && react-native run-ios",
    "android:bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:bundle:dev": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:run:dev": "npm run android:bundle:dev && react-native run-android",
    "web:bundle": "webpack --config web/tools/webpack/webpack.config.prod.js --progress --colors",
    "web:dev": "webpack-dev-server --config web/tools/webpack/webpack.config.dev.js --inline --colors",
    "clear-cache": "gulp clear-cache"
  },
  "dependencies": {
    "axios": "0.15.3",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-native": "0.41.2",
    "react-native-web": "0.0.70",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-saga": "0.14.3"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "4.0.0",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-umd": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.7.0",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "html-webpack-plugin": "2.28.0",
    "ignore-styles": "5.0.1",
    "img-loader": "1.3.1",
    "istanbul": "0.4.5",
    "jsdom": "9.9.1",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "node-sass": "4.2.0",
    "nyc": "10.1.2",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "1.3.1",
    "react-native-cli": "2.0.1",
    "react-native-css-loader": "1.0.2",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "15.4.2",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.3",
    "redux": "3.6.0",
    "redux-devtools": "3.3.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "redux-logger": "2.8.1",
    "resolve-url-loader": "1.6.1",
    "sass-loader": "4.1.1",
    "source-map-loader": "0.1.5",
    "source-map-support": "0.4.8",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-hot-middleware": "2.16.1",
    "webpack-node-externals": "1.5.4"
  }
}
